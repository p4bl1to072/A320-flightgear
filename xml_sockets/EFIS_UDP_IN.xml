<PropertyList>
    <generic>
        <input>
                <line_separator>\n</line_separator>
                <var_separator>,</var_separator>
            <chunk>
                <name>efis_button_pressed</name>
                <type>string</type>
                <format>%s</format>
                <node>/input/arduino/EFIS/button_pressed</node>
            </chunk>

            <chunk>
                <name>button_WPT</name>
                <type>bool</type>
                <format>%d</format>
                <node>/instrumentation/efis/inputs/wpt</node>
            </chunk>
            <chunk>
                <name>button_VORD</name>
                <type>bool</type>
                <format>%d</format>
                <node>/instrumentation/efis/inputs/VORD</node>
            </chunk>
            <chunk>
                <name>button_NDB</name>
                <type>bool</type>
                <format>%d</format>
                <node>/instrumentation/efis/inputs/NDB</node>
            </chunk>
            <chunk>
                <name>button_ARPT</name>
                <type>bool</type>
                <format>%d</format>
                <node>/instrumentation/efis/inputs/arpt</node>
            </chunk>

            <chunk>
                <name>setting_hPa</name>
                <type>double</type>
                <format>%10.5f</format>
                <node>/instrumentation/altimeter/setting-hpa</node>
            </chunk>
            <chunk>
                <name>setting_Hg</name>
                <type>double</type>
                <format>%5.2f</format>
                <node>/instrumentation/altimeter/setting-hg</node>
            </chunk>

            <chunk>
                <name>my_knob_display</name>
                <type>string</type>
                <format>%s</format>
                <node>/instrumentation/efis/inputs/range-nm</node>
            </chunk>
            <chunk>
                <name>range_NM</name>
                <type>int</type>
                <format>%3d</format>
                <node>/instrumentation/efis/inputs/range-nm</node>
            </chunk>
            <chunk>
                <name>3pos_left</name>
                <type>int</type>
                <format>%+2d</format>
                <node>/instrumentation/efis/inputs/lh-vor-adf</node>
            </chunk>
            <chunk>
                <name>3pos_right</name>
                <type>int</type>
                <format>%+2d</format>
                <node>/instrumentation/efis/inputs/rh-vor-adf</node>
            </chunk>
            <chunk>
                <name>button_Hg-hPa</name>
                <type>bool</type>
                <format>%d</format>
                <node>/instrumentation/altimeter/inhg</node>
            </chunk>
            <chunk>
                <name>button_std_altimeter</name>
                <type>bool</type>
                <format>%d</format>
                <node>/instrumentation/altimeter/std</node>
            </chunk>

        </input>
    </generic>
</PropertyList>
